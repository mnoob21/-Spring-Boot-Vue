<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.auggie.student_server.mapper.AdminMapper">
    <select id="findByPhone" resultType="com.auggie.student_server.entity.Admin">
        select * from studentms.a
        <where>
                aphone = #{aphone}
        </where>
    </select>
</mapper>
